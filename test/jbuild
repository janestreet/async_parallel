(alias
 ((name runtest)
  (deps (qtest.exe test_remote.exe))
  (action (bash "./qtest.exe"))))

(executables
 ((names (qtest test_remote))
  (libraries (async
              async_parallel_deprecated
              core
              qtest_lib))
  (preprocess (pps (ppx_jane ppxlib.runner)))))


(jbuild_version 1)
